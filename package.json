{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "A basic typescript app starter for 2024.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:tsc": "rimraf ./dist && tsc",
    "build": "rimraf ./dist && swc ./src --strip-leading-paths -d dist",
    "build:watch": "swc ./src --strip-leading-paths -d dist --watch",
    "start": "node --enable-source-maps dist/index.js",
    "browser": "npm run build && node dist/browser.js",
    "lint": "eslint ./src --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.5",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.6.1",
    "@types/tcp-ping": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^28.0.7",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@adonisjs/transmit-client": "^1.0.0",
    "@redux-devtools/remote": "^0.9.3",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/eventsource": "^1.1.15",
    "bufferutil": "^4.0.8",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "eventsource": "^2.0.2",
    "jest-cucumber": "^3.0.1",
    "kind-of": "^6.0.3",
    "moment": "^2.30.1",
    "puppeteer": "^22.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "socket.io-client": "^4.7.5",
    "string-replace-loader": "^3.1.0",
    "tcp-ping": "^0.1.1",
    "typedi": "^0.10.0",
    "utf-8-validate": "^6.0.4"
  },
  "packageManager": "yarn@4.3.1"
}
